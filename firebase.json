{
  "hosting": {
    "target": "nouvewear",
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/checkout",
        "destination": "/checkout.html"
      },
      {
        "source": "/purchase",
        "destination": "/purchase.html"
      },
      {
        "source": "/thanks",
        "destination": "/thanks.html"
      },
      {
        "source": "/faq",
        "destination": "/faq.html"
      },
      {
        "source": "/terms",
        "destination": "/terms.html"
      },
      {
        "source": "/return",
        "destination": "/return.html"
      },
      {
        "source": "/categories",
        "destination": "/categories.html"
      },
      {
        "source": "/shipping",
        "destination": "/shipping.html"
      },
      {
        "source": "/tracking",
        "destination": "/tracking.html"
      },
      {
        "source": "/about",
        "destination": "/about.html"
      },
      {
        "source": "/shop",
        "destination": "/shop.html"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ]
  }
}
